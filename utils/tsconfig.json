{
  "include": ["./src/**/*"],
  "exclude": ["utils/node_modules"],
  "compilerOptions": {
    "types": ["jest", "node", "@types/jest"],
    "target": "es5",
    "module": "commonjs",
    "outDir": "./out",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
